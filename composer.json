    "symfony/routing": "3.4.36",
    "symfony/http-foundation": "~2.8",
    "symfony/yaml": "~4.0",
    "symfony/expression-language": "~3.0",
    "symfony/config": "3.3.2",
    "twig/twig": "1.*",
    "hybridauth/hybridauth": "^3.2",
    "michelf/php-markdown": "^1.9"
